{
  "extends": "../tsconfig.settings.json",
  "include": [
    "../packages/**/*.d.ts"
  ],
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "composite": false,
    "allowJs": true,
    "noUnusedLocals": false,
    "downlevelIteration": true,
    "lib": [
      "dom",
      "es2019"
    ],
    "paths": {
      "react": [
        "./node_modules/@types/react/index"
      ]
    }
  }
}
